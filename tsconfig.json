{
    "compilerOptions": {
        "module": "es6",
        "target": "es6",
        "noImplicitAny": false,
        "sourceMap": false,
        "charset": "utf-8",
        "watch": true,
        "outDir": "./js/compiled-js/"
    },
    "include": [
        "./ts/**/*.ts"
    ],
    "compileOnSave": true,
    "typeAcquisition": {
        "enable": true
    }
}